{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "ignorePatterns": [
  ],
  "rules": {
      "indent": [
          "warn",
          2,
          {
            "SwitchCase": 1
          }
      ],
      "linebreak-style": [
          "warn",
          "unix"
      ],
      "quotes": [
          "warn",
          "single"
      ],
      "semi": [
          "warn",
          "always"
      ],
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "after-used",
          "ignoreRestSiblings": false
        }
      ],
      "no-console": [
        "warn",
        {
          "allow": ["warn", "error"]
        }
      ]
  }
}